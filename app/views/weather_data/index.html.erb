<p id="notice"><%= notice %></p>

<h1>Weather Data</h1>

<table>
  <thead>
    <tr>
      <th>Data</th>
      <th>Hora</th>
      <th>Prec total horario</th>
      <th>Pres atmos nivel estacao</th>
      <th>Pres atmos max</th>
      <th>Pres atmos min</th>
      <th>Radiacao global</th>
      <th>Temp ar bulso seco</th>
      <th>Temp ponto orvalho</th>
      <th>Temp min hora ant</th>
      <th>Temp monima hora ant</th>
      <th>Temp orvalho max hora ant</th>
      <th>Temp orvalho min hora ant</th>
      <th>Umid rel max hora ant</th>
      <th>Umid rel min hora ant</th>
      <th>Umid relativa ar</th>
      <th>Vento direcao horario</th>
      <th>Vento rajada maxima</th>
      <th>Vento velocidade horario</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @weather_data.each do |weather_datum| %>
      <tr>
        <td><%= weather_datum.data %></td>
        <td><%= weather_datum.hora %></td>
        <td><%= weather_datum.prec_total_horario %></td>
        <td><%= weather_datum.pres_atmos_nivel_estacao %></td>
        <td><%= weather_datum.pres_atmos_max %></td>
        <td><%= weather_datum.pres_atmos_min %></td>
        <td><%= weather_datum.radiacao_global %></td>
        <td><%= weather_datum.temp_ar_bulso_seco %></td>
        <td><%= weather_datum.temp_ponto_orvalho %></td>
        <td><%= weather_datum.temp_min_hora_ant %></td>
        <td><%= weather_datum.temp_monima_hora_ant %></td>
        <td><%= weather_datum.temp_orvalho_max_hora_ant %></td>
        <td><%= weather_datum.temp_orvalho_min_hora_ant %></td>
        <td><%= weather_datum.umid_rel_max_hora_ant %></td>
        <td><%= weather_datum.umid_rel_min_hora_ant %></td>
        <td><%= weather_datum.umid_relativa_ar %></td>
        <td><%= weather_datum.vento_direcao_horario %></td>
        <td><%= weather_datum.vento_rajada_maxima %></td>
        <td><%= weather_datum.vento_velocidade_horario %></td>
        <td><%= link_to 'Show', weather_datum %></td>
        <td><%= link_to 'Edit', edit_weather_datum_path(weather_datum) %></td>
        <td><%= link_to 'Destroy', weather_datum, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Weather Datum', new_weather_datum_path %>
